{
  "name": "cat-energy",
  "version": "14.0.0",
  "private": true,
  "description": "Личный проект «Кэт энерджи» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "browser-sync": "2.26.3",
    "del": "4.1.0",
    "gulp": "4.0.0",
    "gulp-autoprefixer": "6.0.0",
    "gulp-clean-css": "4.0.0",
    "gulp-compile-handlebars": "0.6.1",
    "gulp-concat": "2.6.1",
    "gulp-imagemin": "5.0.3",
    "gulp-rename": "1.4.0",
    "gulp-sass": "4.0.1",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svg-sprite": "1.5.0",
    "gulp-uglify": "3.0.2",
    "gulp-wait": "0.0.2",
    "gulp-watch": "5.0.1",
    "gulp-webp": "4.0.1",
    "imagemin-webp": "5.0.0",
    "pump": "3.0.0",
    "stylelint": "9.10.1",
    "stylelint-config-htmlacademy": "0.1.3"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/script.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "10.15"
  }
}
